#include <iostream>
#include <cmath>
using namespace std;

bool circulo(int raio, int x, int y, int cx, int cy ){
    cout << (cx - x)*(cx-x) + (cy - y)*(cy-y)<< endl;
    cout << raio*raio << endl;
    if(( (cx - x)*(cx-x) + (cy - y)*(cy-y)) <= raio*raio)

            return true;
    else
        return false;
}
int Raio, centro_x, centro_y, ponto_x, ponto_y;
char saida;
int main()
{
    cout << "Raio:" << endl;
    cin >> Raio;
    cout << "Centro_ x:" << endl;
    cin >> centro_x;
    cout << "Centro_ y:" << endl;
    cin>> centro_y;
    cout << "Ponto_ x:" << endl;
    cin>> ponto_x ;
    cout << "Ponto_ y:" << endl;
    cin>> ponto_y;

    if(circulo(Raio,ponto_x, ponto_y, centro_x, centro_y))
        cout << "Esta dentro do circulo" << endl;
    else
        cout << "NÃ£o esta dentro do circulo"<< endl;
    return 0;
}
